{
    "name": "muensmedia/hyvor-relay",
    "description": "A Laravel package for using the hyvor-relay",
    "type": "library",
    "require": {
        "php": "^8.5",
        "lorisleiva/laravel-actions": "^2.9",
        "spatie/laravel-package-tools": "^1.92",
        "spatie/laravel-data": "^4.19",
        "symfony/http-client": "^8.0",
        "guzzlehttp/guzzle": "^7.10",
        "laravel/framework": "^12"
    },
    "require-dev": {
        "laravel/tinker": "^2.11",
        "orchestra/testbench": "^10.9",
        "pestphp/pest": "^4.3",
        "pestphp/pest-plugin-arch": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Muensmedia\\HyvorRelay\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Muensmedia\\HyvorRelay\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Muensmedia\\HyvorRelay\\HyvorRelayServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage --configuration phpunit.coverage.xml.dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Iven Schlenther",
            "email": "i.schlenther@muensmedia.de"
        }
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
